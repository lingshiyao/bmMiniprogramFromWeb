<view class="phone-select-tool-bar">
    <view class="phone-select-list">
        <view class="phone-select-item" wx:for="{{dataEntity}}" wx:for-item="item" wx:for-index="index"
              bindtap="tabberToolAction" data-index="{{index}}" wx:key="unique">
            <view class="phone-select-title" style="{{item.isSelect?'color: #1E1E1E':'#646464'}}">
                {{ item.title }}
            </view>
            <view wx:if="{{item.isSelect == true}}" class="phone-select-bottom-line"></view>
        </view>
    </view>
</view>