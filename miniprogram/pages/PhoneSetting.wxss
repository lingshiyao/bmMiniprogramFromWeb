
.setting {
    display: flex;
    flex-direction: column;
    align-items: center;
    background: #ffffff;
    min-height: 100vh;
}

.top {
    width: 100%;
    position: relative;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    height: 11.73vw;
}

.back-arrow {
    width: 5.47vw;
    height: 4.67vw;
    position: absolute;
    left: 4vw;
}

.title {
    font-size: 4.8vw;
    font-weight: bold;
    color: #111111;
    position: absolute;
}

.text {
    font-size: 4vw;
    color: #323232;
    margin-left: 3.87vw;
    font-weight: 500;
}

.line {
    display: flex;
    justify-content: space-between;
    width: 100vw;
    align-items: center;
}

.header-box {
    width: 12.8vw;
    height: 12.8vw;
    overflow: hidden;
    border-radius: 6.4vw;
    position: relative;
    display: flex;
    justify-content: center;
    z-index: 2;
    background: #131c43;
}

.header {
    position: absolute;
    background: #131c43;
    border-radius: 6.4vw;
}

.right-arrow {
    width: 1.6vw;
    height: 3.33vw;
    margin-right: 4vw;
    margin-left: 3.47vw;
}

.input-line {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: flex-end;
    flex: 1;
}

.input-text {
    font-size: 4vw;
    font-weight: 500;
    color: #f99d1a;
}

.input-text-has {
    font-size: 4vw;
    font-weight: 500;
    color: #969696;
    flex: 1;
    text-align: end;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: clip;
    width: 10vw;
    margin-left: 5vw;
}

.line-h {
    width: 92vw;
    height: 0.27vw;
    background: #f0f0f0;
}

.login-out {
    width: 92vw;
    height: 11.73vw;
    background: #eeeeee;
    border-radius: 3vw;
    font-size: 4vw;
    font-weight: bold;
    color: #ff4b36;
    line-height: 11.73vw;
    text-align: center;
}

.file-ele {
    position: absolute;
    display: flex;
    overflow: hidden;
    opacity: 0;
    width: 25.6vw;
    height: 12.8vw;
    background: #00a3fb;
    cursor: pointer;
}

.hidden {
    visibility: hidden;
}
